<!-- Elevator trim -->
<animation>
<type>pick</type>
<object-name>ElevatorTrim</object-name>
<action>
<button>0</button>
<button>3</button>
<repeatable type="bool">true</repeatable>
<binding>
<command>property-adjust</command>
<property>controls/flight/elevator-trim</property>
<step>0.01</step>
<max>1</max>
</binding>
</action>
<action>
<button>1</button>
<button>4</button>
<repeatable type="bool">true</repeatable>
<binding>
<command>property-adjust</command>
<property>controls/flight/elevator-trim</property>
<step>-0.01</step>
<min>0</min>
</binding>
</action>
</animation>
<animation>
<type>rotate</type>
<object-name>ElevatorTrim</object-name>
<property>controls/flight/elevator-trim</property>
<interpolation>
<entry><ind>-1</ind><dep>56</dep></entry>
<entry><ind>0</ind><dep>12.5</dep></entry>
<entry><ind>1</ind><dep>3</dep></entry>
</interpolation>
<axis>
<x>0</x>
<y>1</y>
<z>0</z>
</axis>
<center>
<x-m>-2.548</x-m>
<y-m>0</y-m>
<z-m>0.522</z-m>
</center>
</animation>

<!-- Speedbrake/spoiler lever -->
<animation>
<type>pick</type>
<object-name>SpeedbrakeLever</object-name>
<action>
<button>0</button>
<repeatable type="bool">false</repeatable>
<binding>
<command>property-adjust</command>
<property>controls/flight/spoilers</property>
<step>0.2</step>
<max>1</max>
</binding>
</action>
<action>
<button>1</button>
<repeatable type="bool">false</repeatable>
<binding>
<command>property-adjust</command>
<property>controls/flight/spoilers</property>
<step>-0.2</step>
<min>0</min>
</binding>
</action>
</animation>
<animation>
<type>rotate</type>
<object-name>SpeedbrakeLever</object-name>
<property>controls/flight/spoilers</property>
<interpolation>
<entry><ind>0</ind><dep>-33</dep></entry>
<entry><ind>0.2</ind><dep>2</dep></entry>
<entry><ind>0.4</ind><dep>10</dep></entry>
