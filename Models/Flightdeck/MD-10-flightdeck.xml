<PropertyList>

<path>MD-10-flightdeck.ac</path>
<!--Working For Rembrandt-->

<animation>
<object-name>Glass</object-name>
</animation>

<animation>
<type>noshadow</type>
<object-name>Glass</object-name>
</animation>

<animation>
<type>shader</type>
<shader>chrome</shader>
<texture>Aircraft/Generic/Effects/glass_shader.png</texture>
<object-name>Glass</object-name>
</animation>
<!--Working For Rembrandt-->

<animation>
<type>range</type>
<min-m>0</min-m>
<max-m>2000</max-m>
</animation>

